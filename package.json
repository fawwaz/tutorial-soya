    "build:testing": "SOYA_PROJECT_DIR=$PWD NODE_ENV='testing' webpack --bail",
    "build:production": "SOYA_PROJECT_DIR=$PWD NODE_ENV='production' webpack --bail",
    "run": "SOYA_PROJECT_DIR=$PWD node build/server",
    "nodemon": "SOYA_PROJECT_DIR=$PWD nodemon ./build/server",
    "clean": "rm -rf build/",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
